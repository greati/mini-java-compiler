program SwitchExample;

class Switch {

    method string map(val int key) {
        
        switch key {
            case 1 {
                return "A"
            }
            case 2 {
                return "B"
            }
            case 3 {
                return "C"
            }
            default {
                return "Invalid"
            }
        };

        print "Finished."
    }
}
