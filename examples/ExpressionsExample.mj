program Expressions;

class Expr {

    declarations
        MyClass[] v = @MyClass[10]; 
        int a, b;
        int bool;
    enddeclarations

    method void makeExpr() {
        read a;
        read b;   
    
        bool := a && b;
        bool := a || b;
        bool := a + b - a * b / a % b;
        bool := a <= b;
        a := -2;
        bool := a + v[2].name[19]
    }
}
